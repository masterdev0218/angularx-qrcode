{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "angularx-qrcode",
  "version": "2.0.1",
  "author": {
    "name": "Andreas Jacob <andreas@cordobo.de>",
    "url": "http://cordobo.com/"
  },
  "description": "Simple QRCode module generator for Angular4/5/6/7/8+ and Ionic 3/4 using node-qrcode",
  "license": "MIT",
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "lint": "tslint 'src/**/*.ts' --fix",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/angularx-qrcode && npm publish && cd ../../"
  },
  "dependencies": {
    "@angular/common": ">= 5.0.0 < 9.0.0",
    "qrcode": "1.4.2"
  },
  "peerDependencies": {
    "@angular/common": ">= 5.0.0 < 9.0.0",
    "@angular/core": ">= 5.0.0 < 9.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.21",
    "@angular-devkit/build-ng-packagr": "0.803.21",
    "@angular/cli": "8.3.21",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@types/node": "8.9.4",
    "@types/qrcode": "1.3.4",
    "codelyzer": "5.2.1",
    "ng-packagr": "5.7.1",
    "rimraf": "3.0.0",
    "rxjs": "6.4.0",
    "ts-node": "8.5.4",
    "tslib": "1.10.0",
    "tslint": "5.20.1",
    "typescript": "~3.5.3",
    "zone.js": "0.10.2"
  },
  "keywords": [
    "angularx-qrcode",
    "angular-qrcode",
    "ng-qrcode",
    "ngx-qrcode",
    "angular",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "ionic",
    "ionic3",
    "ionic4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cordobo/angularx-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/cordobo/angularx-qrcode/issues"
  },
  "homepage": "https://github.com/cordobo/angularx-qrcode#readme"
}
